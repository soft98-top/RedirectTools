{
    "manifest_version":2,
    "name":"RedirectTools",
    "version":"1.2.0",
    "description":"网页重定向工具",
    "icons":{
        "16":"img/icon.png",
        "48":"img/icon.png",
        "128":"img/icon.png"
    },
    "background":{
        "page":"background.html"
    },
    "browser_action":{
        "default_icon":"img/icon.png",
        "default_title":"WebSiteRedirect - Soft98",
        "default_popup":"popup.html"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["js/jquery-3.5.1.js"],
            "run_at":"document_start"
        }
    ],
    "permissions":[
        "proxy",
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "homepage_url":"https://www.soft98.top"
}