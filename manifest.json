{
    "manifest_version":2,
    "name":"RedirectTools",
    "version":"1.3.2",
    "description":"重定向综合工具-重定向，修改返回值，代理等",
    "icons":{
        "16":"img/icon.png",
        "48":"img/icon.png",
        "128":"img/icon.png"
    },
    "background":{
        "page":"background.html"
    },
    "browser_action":{
        "default_icon":"img/icon.png",
        "default_title":"RedirectTools by Soft98",
        "default_popup":"popup.html"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["js/jquery-3.5.1.js"],
            "run_at":"document_start"
        }
    ],
    "permissions":[
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "homepage_url":"https://github.com/soft98-top/RedirectTools"
}